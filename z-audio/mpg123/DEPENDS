case ${MPG123_AUDIO} in
  esd)
    depends esound
  ;;
  jack)
    depends jack
  ;;
  libao)
    depends libao
  ;;
  oss)
  ;;
  portaudio)
    depends portaudio
  ;;
  nas)
    depends nas
  ;;
  alsa)
    depends alsa-lib
  ;;
  sdl)
    depends sdl
  ;;
  dummy)
    message "building mpg123 without audio device output (only raw and wav/au)"
  ;;
esac
