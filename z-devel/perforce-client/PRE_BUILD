mk_source_dir	$SOURCE_DIRECTORY &&
unpack_file '' &&
FILENAME=`guess_filename $SOURCE_CACHE/$SOURCE` &&
cp $FILENAME $SOURCE_DIRECTORY &&

unpack_file 2 &&
FILENAME=`guess_filename $SOURCE_CACHE/$SOURCE2` &&
cp $FILENAME $SOURCE_DIRECTORY

unpack_file 3 &&
FILENAME=`guess_filename $SOURCE_CACHE/$SOURCE3` &&
cp $FILENAME $SOURCE_DIRECTORY
