mk_source_dir  ${SOURCE_DIRECTORY}        &&
cd             ${SOURCE_DIRECTORY}        &&
unpack         ${SOURCE}  ${MD5[0]}       &&
rm  -rf  $SOURCE_DIRECTORY/old.libraries  &&
patch  $SOURCE_DIRECTORY/usr/lib/transgaming_point2play/setup_wizard_gui.py  \
       $SCRIPT_DIRECTORY/system_info.patch
