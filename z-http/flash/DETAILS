           SPELL=flash
if [[ $FLASH_BETA == y ]]; then
         VERSION=051508
          SOURCE=flashplayer10_install_linux_$VERSION.tar.gz
   SOURCE_URL[0]=http://download.macromedia.com/pub/labs/flashplayer10/$SOURCE
SOURCE_DIRECTORY=$BUILD_DIRECTORY/install_flash_player_10_linux
else
         VERSION=9.0.124.0
  SECURITY_PATCH=3
          SOURCE=install_flash_player_9_linux.tar.gz
   SOURCE_URL[0]=http://fpdownload.macromedia.com/get/flashplayer/current/$SOURCE
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${SOURCE/.tar.gz/}
fi
  FORCE_DOWNLOAD="on"
      SOURCE_GPG=gurus.gpg:$SOURCE.sig
