           SPELL=ipw2100 
         VERSION=1.1.0
       FWVERSION=1.3
       BUILD_API=2
          SOURCE=$SPELL-$VERSION.tgz
	 SOURCE2=$SPELL-fw-$FWVERSION.tgz
      SOURCE_GPG=gurus.gpg:${SOURCE}.sig
     SOURCE2_GPG=gurus.gpg:${SOURCE2}.sig
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$SPELL-$VERSION
   SOURCE_URL[0]=$SOURCEFORGE_URL/$SPELL/$SOURCE   
  SOURCE2_URL[0]=
        WEB_SITE=http://$SPELL.sf.net/
         ENTERED=20040815
         UPDATED=20040919
      LICENCE[0]=GPL
          REJECT="Firmware must be downloaded manually, license agreement"
         ARCHIVE="off"
           SHORT="Intel ProWireless 2100 kernel drivers"
cat << EOF
This is the Intel Pro Wireless 2100 driver found on the website ipw2100.sf.net if you want more info than that check out the web site
EOF
