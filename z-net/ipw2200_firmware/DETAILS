           SPELL=ipw2200_firmware  
         VERSION=$FIRMWARE_VERSION
          SOURCE=ipw2200-fw-$VERSION.tgz
SOURCE_DIRECTORY=$BUILD_DIRECTORY/ipw2200-fw
   SOURCE_URL[0]=
      SOURCE_GPG="gurus.gpg:${SOURCE}.sig:WORKS_FOR_ME"
        WEB_SITE=http://ipw2200.sf.net/
         ENTERED=20040815
      LICENCE[0]=http://ipw2200.sourceforge.net/firmware.php
          REJECT="has licensing issues you must download it from the website"
         ARCHIVE="off"
           SHORT="Intel ProWireless 2200 firmware"
cat << EOF
This is the Intel Pro Wireless 2200 firmware found on the website ipw2200.sf.net if you want more info than that check out the web site
EOF
