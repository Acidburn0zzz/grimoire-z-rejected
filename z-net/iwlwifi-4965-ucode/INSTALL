message  "${MESSAGE_COLOR}Installing firmware...$DEFAULT_COLOR"                         &&
mkdir -p  $INSTALL_ROOT/lib/firmware                                                    &&
cp -v $SPELL-${VERSION/2.21/1.21/}/${SPELL/-ucode/}-1.ucode $INSTALL_ROOT/lib/firmware  &&
