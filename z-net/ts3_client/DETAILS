          SOURCE=TeamSpeak3-Client-linux_amd64-${VERSION}.run
else
          SOURCE=TeamSpeak3-Client-linux_x86-${VERSION}.run
fi
      SOURCE_GPG="gurus.gpg:$SOURCE.sig:WORKS_FOR_ME"
